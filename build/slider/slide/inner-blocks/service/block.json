{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ross/service",
  "version": "0.1.0",
  "title": "Service",
  "category": "theme",
  "icon": "businessman",
  "example": {},
  "attributes": {
    "mediaID": {
      "type": "number"
    },
    "mediaURL": {
      "type": "string",
      "source": "attribute",
      "selector": ".wp-block-ross-service__image",
      "attribute": "src"
    },
    "pageID": {
      "type": "string",
      "default": "0"
    },
    "link": {
      "type": "string"
    },
    "label": {
      "type": "string"
    },
    "bgColor": {
      "type": "string"
    },
    "textColor": {
      "type": "string"
    }
  },
  "supports": {
    "html": false,
    "className": false
  },
  "parent": [
    "ross/slider-slide"
  ],
  "textdomain": "ross",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}