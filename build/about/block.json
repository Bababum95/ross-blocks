{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ross/about",
  "version": "0.1.0",
  "title": "About us",
  "category": "theme",
  "icon": "universal-access",
  "example": {},
  "attributes": {
    "mediaID": {
      "type": "number"
    },
    "mediaURL": {
      "type": "string",
      "source": "attribute",
      "selector": ".wp-block-ross-about__image",
      "attribute": "src"
    },
    "title": {
      "type": "string",
      "default": "Reliable & Excellence service"
    },
    "description": {
      "type": "string",
      "default": ""
    },
    "layout": {
      "type": "number",
      "default": 1
    },
    "highlights": {
      "type": "array",
      "default": [
        {
          "value": "99%",
          "description": "Customer satisfaction rate"
        },
        {
          "value": "5000+",
          "description": "Satisfied clients"
        },
        {
          "value": "200+",
          "description": "5-Star reviews from satisfied customers"
        },
        {
          "value": "85%",
          "description": "of repairs are done on the same day"
        }
      ]
    },
    "licenseNumber": {
      "type": "string",
      "default": "â„–#A 50630"
    },
    "licenseLink": {
      "type": "string",
      "default": ""
    },
    "blockName": {
      "type": "boolean",
      "default": true
    }
  },
  "supports": {
    "html": false,
    "className": false
  },
  "textdomain": "ross",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}